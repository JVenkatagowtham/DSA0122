#include <iostream>
using namespace std;

int main() {
    int num, sum = 0, digit;
    cout << "Enter a number: ";
    cin >> num;

    while (num != 0) {
        digit = num % 10; // Extract the last digit
        sum += digit;     // Add the digit to sum
        num /= 10;        // Remove the last digit from the number
    }

    cout << "Sum of digits: " << sum;

    return 0;
}

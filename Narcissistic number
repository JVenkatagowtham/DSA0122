#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int num, numDigits, originalNum, digit, sum = 0;

    cout << "Enter a positive integer: ";
    cin >> num;

    originalNum = num;
    numDigits = log10(num) + 1;

    while (num > 0) {
        digit = num % 10;
        sum += pow(digit, numDigits);
        num /= 10;
    }

    if (sum == originalNum)
        cout << originalNum << " is a narcissistic number.";
    else
        cout << originalNum << " is not a narcissistic number.";

    return 0;
}

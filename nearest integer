#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double num;
    cout << "Enter a floating-point number: ";
    cin >> num;

    cout << "Rounded to the nearest integer using floor function: " << floor(num) << endl;
    cout << "Rounded to the nearest integer using ceil function: " << ceil(num) << endl;

    return 0;
}
